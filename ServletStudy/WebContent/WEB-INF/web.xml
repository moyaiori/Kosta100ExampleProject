<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>ServletStudy</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
    <servlet-name>HelloServlet</servlet-name>
    <servlet-class>kr.or.kosta.servlet.HelloServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet</servlet-name>
    <url-pattern>/hello</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>GugudanServlet</display-name>
    <servlet-name>GugudanServlet</servlet-name>
    <servlet-class>kr.or.kosta.servlet.GugudanServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>GugudanServlet</servlet-name>
    <url-pattern>/gugudan</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>MIMETypeServlet</display-name>
    <servlet-name>MIMETypeServlet</servlet-name>
    <servlet-class>kr.or.kosta.servlet.MIMETypeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MIMETypeServlet</servlet-name>
    <url-pattern>/mp3Download</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <description></description>
    <display-name>LifeCyleServlet</display-name>
    <servlet-name>LifeCyleServlet</servlet-name>
    <servlet-class>kr.or.kosta.servlet.LifeCyleServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LifeCyleServlet</servlet-name>
    <url-pattern>/lifecyle</url-pattern>
  </servlet-mapping>
</web-app>